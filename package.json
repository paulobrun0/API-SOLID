{
  "name": "api-solid",
  "version": "1.0.0",
  "description": "GymPass style app.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "test:create-prisma-environment": "npm install ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm install vitest-environment-prisma",
    "build": "tsup src --out-dir build",
    "lint": "eslint . --ext .ts --fix",
    "test": "vitest --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest  --dir src/http",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@prisma/client": "6.5.0",
    "@rocketseat/eslint-config": "2.2.2",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "22.13.5",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "3.0.7",
    "@vitest/ui": "3.0.7",
    "eslint": "8.57.1",
    "i": "0.3.7",
    "npm": "11.2.0",
    "npm-run-all": "4.1.5",
    "prisma": "6.5.0",
    "supertest": "7.0.0",
    "tsup": "8.3.6",
    "tsx": "4.19.3",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.7"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/jwt": "9.0.4",
    "bcryptjs": "3.0.2",
    "dayjs": "1.11.13",
    "dotenv": "16.4.7",
    "fastify": "5.2.1",
    "zod": "3.24.2"
  }
}
